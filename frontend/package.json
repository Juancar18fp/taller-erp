{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Proyecto de un ERP de un taller para el TFG",
  "productName": "TallerPro",
  "author": "Fajardo <juancar18fp@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run src/**/*.integration.test.ts",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:component": "vitest run src/components/**/*.test.ts",
    "test:verbose": "vitest run --reporter=verbose",
    "test:silent": "vitest run --reporter=basic"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.3",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@pinia/testing": "^1.0.2",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/vite-plugin": "^1.4.0",
    "@types/node": "^20.5.9",
    "@types/papaparse": "^5.3.16",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "happy-dom": "^17.6.3",
    "jsdom": "^24.1.3",
    "msw": "^2.10.2",
    "prettier": "^3.3.3",
    "quasar": "^2.14.0",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.2.2",
    "vue": "^3.3.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
